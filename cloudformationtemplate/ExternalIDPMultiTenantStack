                console.error('2' + JSON.stringify(error));
                  console.log(`error: ${error}\n${error.stack}`);
                  result = {
                      Status: 'Failed',
                      Data: error
                  };
              }
              const response = yield sendResponse(event, context.logStreamName, result.Status, result.Data);
              return true;
          });
